package corejavapackage;

public class O1_Variable {

	public static void main(String[] args) {
		int a; // Declaration
		a = 10; // Initialization
		System.out.println(a);

		int b; // Declaration
		b = 100; // Initialization
		System.out.println(b);

	}
}
